all:
	g++ server.cpp -g -Wall -o server -std=c++11;
	g++ subscriber.cpp -g -Wall -o subscriber -std=c++11;


server:
	g++ server.cpp -g -Wall -o server -std=c++11;

subscriber:
	g++ subscriber.cpp -g -Wall -o subscriber -std=c++11;

clean:
	rm -rf subscriber server
